{
  "name": "zero-based-budgeting-app",
  "version": "1.0.0",
  "description": "AI-powered zero-based budgeting app with Plaid integration",
  "scripts": {
    "dev": "node scripts/deploy.js && next dev -p 3001",
    "build": "node scripts/deploy.js && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "postinstall": "node scripts/deploy.js",
    "db:dev": "cp prisma/schema.dev.prisma prisma/schema.prisma && npx prisma generate",
    "db:prod": "cp prisma/schema.prod.prisma prisma/schema.prisma && npx prisma generate"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.12.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "22.9.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "lucide-react": "0.263.1",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "openai": "^5.10.2",
    "plaid": "^36.0.0",
    "postcss": "8.4.49",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-plaid-link": "^4.0.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.18",
    "prisma": "^6.12.0"
  }
}
